# Default library view mode
set aaa_mode=all
# Dont stop after finishing a song
set continue=true
# Repeat all tracks
set repeat=true
# set shuffle=true
# Play library bu default instead of playlist
set play_library=true
set show_current_bitrate=true

set format_current=%a - %t%= %2n. %l [%y]
set format_trackwin=%2n. %t%= %g %8d
set format_playlist=%-15%A %-20l %2n. %t%= [%y] %-15%g %6d

colorscheme yowish
